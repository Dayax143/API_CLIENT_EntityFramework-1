@model Test

<h2>Create New Record</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Quantity"></label>
        <input asp-for="Quantity" class="form-control" type="text" />
    </div>
    

    @* <div class="form-group">
        <label asp-for="au"></label>
        <input asp-for="Quantity" class="form-control" type="text" />
    </div> *@

    <div class="form-group">
        <label asp-for="Last_update"></label>
        <input asp-for="Last_update" class="form-control" type="datetime-local" />
    </div>

    <button type="submit" class="btn btn-primary">Insert</button>
    <a asp-action="Get" class="btn btn-secondary">Cancel</a>
</form>